<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="apiBaseUrl" value="[ADD YOUR ENVD API BASE URL HERE]"/>
    
    <!-- SSO Auth API Uri -->
    <add key="AuthTokenBaseAddress" value="[ENTER YOUR SSO AUTH SERVER API URI]" />
    <add key="AuthTokenRequestUri" value="connect/token"/>
    
    <!-- Nlis SSO-->
    <add key="Nlis.SSO.Client.Id" value="[YOUR NLIS CLIENT ID]" />
    <add key="Nlis.SSO.Client.Secret" value="[YOUR NLIS CLIENT SECRET]" />
    <add key="Nlis.SSO.Grant.Type" value="[YOUR NLIS GRANT TYPE]" />
    <add key="Nlis.SSO.Scope" value="[YOUR NLIS SCOPE]" />

    <!-- LPA SSO-->
    <add key="Lpa.SSO.Client.Id" value="[YOUR LPA CLIENT ID]" />
    <add key="Lpa.SSO.Client.Secret" value="[YOUR LPA CLIENT SECRET]" />
    <add key="Lpa.SSO.Grant.Type" value="[YOUR LPA GRANT TYPE]" />
    <add key="Lpa.SSO.Scope" value="[YOUR LPA SCOPE]" />
    
    <!-- MyMla SSO-->
    <add key="MyMla.SSO.Client.Id" value="[YOUR MYMLA CLIENT ID]" />
    <add key="MyMla.SSO.Client.Secret" value="[YOUR MYMLA CLIENT SECRET]" />
    <add key="MyMla.SSO.Grant.Type" value="[YOUR MYMLA GRANT TYPE]" />
    <add key="MyMla.SSO.Scope" value="[YOUR MYMLA SCOPE]" />
    
  </appSettings>
</configuration>